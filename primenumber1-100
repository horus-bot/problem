
int main() {
    int i, j ;
    bool prime;
 

    printf("Prime numbers between 1 and 100 are:\n");

    for (i = 2; i <= 100; i++) {
        prime = true;
   

        for (j = 2; j <= i / 2; j++) {
            if (i % j == 0) {
              prime = false;
              break;
            }
        }
        if(prime){
            printf("%d\n",i);
        }
    }
 
}
