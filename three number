#include <stdio.h>

int main() {
    int num1,num2,num3;
    printf("enter the number");
    scanf("%d",&num1);
    printf("enter the number");
    scanf("%d",&num2);
    printf("enter the number");
    scanf("%d",&num3);
    num1>num2&&num1>num3 ? ( printf("%d is the greatest",num1)):
    (printf("%d is  not the greatest\n",num1));
    num2>num1&&num2>num3 ? printf("%d is the greatest",num2):printf("%d is  not the greatest\n",num2);
    num3>num2&&num3>num3 ? printf("%d is the greatest",num3):printf("%d is  not the greatest\n",num3);
 



    return 0;
}
